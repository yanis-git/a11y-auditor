{
  "name": "a11y-auditor",
  "version": "1.0.0",
  "description": "Chromeless bot to check accessibility criteria",
  "scripts": {
    "nx": "nx",
    "watch": "nx run-many --target=watch --all",
    "watch:dashboard": "nx run dashboard:watch",
    "watch:browser": "nx watch browser",
    "watch:auditor": "nx run auditor:watch",
    "lint:update": "nx run-many --target=lint --all --fix",
    "lint": "nx run-many --target=lint --all",
    "lint:browser": "nx lint browser --fix",
    "lint:auditor": "nx lint auditor --fix",
    "lint:dashboard": "nx lint dashboard --fix",
    "test": "nx run-many --target=test --all",
    "test:browser": "nx test browser",
    "test:auditor": "nx test auditor",
    "test:dashboard": "nx test dashboard",
    "build": "nx run-many --target=build --all",
    "build:browser": "nx build browser",
    "build:auditor": "nx build auditor",
    "test:serve": "python3 -m http.server --directory auditor/tests/data",
    "result:serve": "python3 -m http.server --directory results",
    "auditor": "node dist/auditor/main.js",
    "postinstall": "ngcc --properties es2020 browser module main",
    "dashboard:serve": "nx serve dashboard",
    "dashboard:build": "nx build dashboard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanis-git/a11y-auditor.git"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "WCAG"
  ],
  "author": "Yanis GHIDOUCHE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yanis-git/a11y-auditor/issues"
  },
  "homepage": "https://github.com/yanis-git/a11y-auditor#readme",
  "dependencies": {
    "@angular/animations": "15.2.8",
    "@angular/common": "15.2.8",
    "@angular/compiler": "15.2.8",
    "@angular/core": "15.2.8",
    "@angular/forms": "15.2.8",
    "@angular/platform-browser": "15.2.8",
    "@angular/platform-browser-dynamic": "15.2.8",
    "@angular/router": "15.2.8",
    "@axe-core/puppeteer": "^4.4.2",
    "@ngrx/component-store": "15.3.0",
    "@nrwl/angular": "15.9.2",
    "bootstrap": "^5.1.3",
    "puppeteer": "^19.7.1",
    "rxjs": "~7.8.0",
    "ts-command-line-args": "^2.4.2",
    "tslib": "^2.0.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.6",
    "@angular-devkit/core": "15.2.6",
    "@angular-devkit/schematics": "15.2.6",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.0",
    "@angular/compiler-cli": "15.2.8",
    "@angular/language-service": "15.2.8",
    "@nrwl/cli": "15.9.2",
    "@nrwl/cypress": "15.9.2",
    "@nrwl/eslint-plugin-nx": "15.9.2",
    "@nrwl/jest": "15.9.2",
    "@nrwl/linter": "15.9.2",
    "@nrwl/node": "15.9.2",
    "@nrwl/workspace": "15.9.2",
    "@schematics/angular": "15.2.6",
    "@tsconfig/node14": "^1.0.1",
    "@types/expect-puppeteer": "^5.0.3",
    "@types/jest": "29.4.4",
    "@types/jest-environment-puppeteer": "^5.0.0",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "cypress": "^12.6.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jest-preset-angular": "13.0.0",
    "jest-puppeteer": "^7.0.1",
    "nx": "15.9.2",
