{
  "name": "a11y-auditor",
  "version": "1.0.0",
  "description": "Chromeless bot to check accessibility criteria",
  "scripts": {
    "build:browser": "nx build browserkit",
    "watch:browser": "nx watch browserkit",
    "build:auditor": "nx build auditor",
    "watch:auditor": "nx watch auditor",
    "lint:browserkit": "nx lint browserkit",
    "lint:auditor": "nx lint auditor",
    "watch": "nx run-many --target=watch --all",
    "test:browserkit": "true",
    "test:auditor": "nx test auditor",
    "build": "npm run build:browser && npm run build:auditor",
    "test:serve": "python3 -m http.server --directory auditor/tests/data",
    "result:serve": "python3 -m http.server --directory results",
    "start": "node dist/auditor/index.js",
    "test": "npm run test:browserkit && npm run test:auditor",
    "postinstall": "ngcc --properties es2015 browser module main",
    "dashboard:serve": "nx serve dashboard",
    "dashboard:build": "nx build dashboard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanis-git/a11y-auditor.git"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "WCAG"
  ],
  "author": "Yanis GHIDOUCHE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yanis-git/a11y-auditor/issues"
  },
  "homepage": "https://github.com/yanis-git/a11y-auditor#readme",
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@axe-core/puppeteer": "^4.4.2",
    "@nrwl/angular": "^14.0.5",
    "puppeteer": "^13.5.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@nrwl/cli": "14.0.5",
    "@nrwl/cypress": "14.0.5",
    "@nrwl/eslint-plugin-nx": "14.0.5",
    "@nrwl/jest": "14.0.5",
    "@nrwl/linter": "14.0.5",
    "@nrwl/workspace": "14.0.5",
    "@tsconfig/node14": "^1.0.1",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^27.4.1",
    "@types/jest-environment-puppeteer": "^5.0.0",
    "@types/node": "^17.0.23",
    "@types/puppeteer": "^5.4.5",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "cypress": "^9.1.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "^27.5.1",
    "jest-preset-angular": "11.1.1",
    "jest-puppeteer": "^6.1.0",
    "nx": "14.0.5",
    "puppeteer-extra": "^3.2.3",
