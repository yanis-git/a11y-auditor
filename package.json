{
  "name": "a11y-auditor",
  "version": "1.0.0",
  "description": "Chromeless bot to check accessibility criteria",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "tsc",
    "watch": "tsc --watch",
    "serve": "python3 -m http.server --directory tests",
    "start": "node dist/index.js",
    "test": "jest --runInBand --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanis-git/a11y-auditor.git"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "WCAG"
  ],
  "author": "Yanis GHIDOUCHE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yanis-git/a11y-auditor/issues"
  },
  "homepage": "https://github.com/yanis-git/a11y-auditor#readme",
  "dependencies": {
    "puppeteer": "^13.5.2"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/puppeteer": "^5.4.5",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  }
}
