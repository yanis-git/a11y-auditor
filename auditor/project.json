{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "auditor/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/auditor",
        "main": "auditor/src/index.ts",
        "tsConfig": "auditor/tsconfig.json",
        "assets": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "auditor:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["auditor/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/auditor"],
      "options": {
        "jestConfig": "auditor/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
