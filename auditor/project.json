{
  "name": "auditor",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "auditor/src",
  "projectType": "application",
  "targets": {
    "watch": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/auditor",
        "main": "auditor/src/index.ts",
        "tsConfig": "auditor/tsconfig.json",
        "assets": [],
        "target": "node",
        "compiler": "tsc",
        "watch": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/auditor",
        "main": "auditor/src/index.ts",
        "tsConfig": "auditor/tsconfig.json",
        "assets": [],
        "target": "node",
        "compiler": "tsc"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "auditor:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "auditor/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/auditor"
      ],
      "options": {
        "jestConfig": "auditor/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
